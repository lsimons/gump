<?xml version="1.0" ?>

<workspace basedir="D:\jakarta" pkgdir="C:\opt" version="0.3">

  <property name="build.sysclasspath" value="only"/>

  <profile href="profile/gump.xml"/>

  <!-- Project overrides -->

  <project name="jakarta-cactus-22" target="jar"/>
  <project name="xml-xalan2-smoketest" target="jar"/>

  <!-- Repository overrides -->

  <repository name="jakarta" method="ext" user="rubys" path="/home/cvs"/>
  <repository name="xml" method="ext" user="rubys" path="/home/cvs"/>

  <!-- After the bootstrap -->
  <module href="project/gump.xml"/>
  <project name="gump" home="D:\gump">
    <ant>
      <property name="workspace" value="rubypad.xml"/>
      <property name="webstyle" value="stylesheet/jakarta.xsl"/>
      <property name="target" value="win2k"/>
      <property name="extension" value="bat"/>
    </ant>
  </project>

</workspace>
