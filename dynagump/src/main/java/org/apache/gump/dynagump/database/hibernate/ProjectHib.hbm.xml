<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>



	<class name="org.apache.gump.dynagump.database.hibernate.ProjectHib" table="projects">
    	<id name="id" column="id" />
    	<property name="name" column="name"/>
    	<property name="description" column="description"/>
    	<many-to-one name="module" column="module_id" not-null="true"/>
        <set name="projectVersion" inverse="true" >
            <key column="project_id" />
            <one-to-many class="org.apache.gump.dynagump.database.hibernate.ProjectVersionIdHib" />
        </set>
	</class>
	
</hibernate-mapping>
