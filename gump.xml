<?xml version="1.0" ?>

<workspace basedir="/data/gump" 
           jardir="/data/gump/jars/latest"
           pkgdir="/data/package"
           sync="rsync -r -a --delete" version="0.3">

  <property name="build.sysclasspath" value="only"/>

  <profile href="profile/gump.xml"/>

  <!-- After the bootstrap -->
  <module href="project/gump.xml"/>
  <project name="gump">
    <ant>
      <property name="workspace" value="gump.xml"/>
      <property name="webstyle" value="stylesheet/jakarta.xsl"/>
      <property name="target" value="bash"/>
      <property name="extension" value="sh"/>
    </ant>
  </project>

</workspace>
