<?xml version="1.0" encoding="UTF-8"?>

<module name="jakarta-turbine-fulcrum">
  <description>Services Framework</description>
  <url href="http://jakarta.apache.org/turbine/fulcrum/">
  </url>
  <cvs module="jakarta-turbine-fulcrum" repository="jakarta">
  </cvs>
  <project name="jakarta-turbine-fulcrum">
    <ant buildfile="build.xml" target="dist">
      <property name="final.name" value="fulcrum-@@DATE@@">
      </property>
    </ant>
    <package>org.apache.fulcrum</package>
    <depend project="avalon-framework" id="combined">
    </depend>
    <depend project="bsf">
    </depend>
    <depend project="commons-codec">
    </depend>
    <depend project="commons-collections">
    </depend>
    <depend project="commons-configuration">
    </depend>
    <depend project="commons-email">
    </depend>
    <depend project="commons-fileupload">
    </depend>
    <depend project="commons-lang">
    </depend>
    <depend project="commons-logging">
    </depend>
    <depend project="commons-pool">
    </depend>
    <depend project="commons-util">
    </depend>
    <depend project="cryptix">
    </depend>
    <depend project="dom4j">
    </depend>
    <depend project="excalibur-compatibility">
    </depend>
    <depend project="excalibur-component">
    </depend>
    <depend project="excalibur-logger">
    </depend>
    <depend project="excalibur-pool">
    </depend>
    <depend project="jaf">
    </depend>
    <depend project="ant">
    </depend>
    <depend project="javamail">
    </depend>
    <depend project="jdbc">
    </depend>
    <depend project="jndi">
    </depend>
    <depend project="junit">
    </depend>
    <depend project="jakarta-log4j">
    </depend>
    <depend project="avalon-logkit">
    </depend>
    <depend project="jakarta-regexp">
    </depend>
    <depend project="jakarta-servletapi">
    </depend>
    <depend project="jakarta-turbine-stratum">
    </depend>
    <depend project="db-torque">
    </depend>
    <depend project="jakarta-velocity">
    </depend>
    <depend project="jakarta-velocity-dvsl">
    </depend>
    <depend project="village">
    </depend>
    <depend project="xml-xalan2">
    </depend>
    <depend project="xml-xerces">
    </depend>
    <depend project="xml-apis">
    </depend>
    <depend project="xml-xerces">
    </depend>
    <depend project="xml-rpc">
    </depend>
    <work nested="target/classes">
    </work>
    <home nested="target">
    </home>
    <jar name="fulcrum-@@DATE@@.jar">
    </jar>
    <javadoc module="jakarta-turbine-fulcrum" nested="target/docs/apidocs">
    </javadoc>
    <nag to="turbine-dev@jakarta.apache.org" from="fulcrum development &lt;turbine-dev@jakarta.apache.org&gt;">
    </nag>
  </project>
</module>
