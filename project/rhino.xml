<module name="rhino">

  <url href="http://www.mozilla.org/rhino/"/>
  <description>
    JavaScript for Java
  </description>

  <cvs repository="mozilla" module="mozilla/js/rhino"/>

  <project name="rhino">
    <ant target="dist">
      <property name="version" value="_gump"/>
    </ant>

    <depend project="jakarta-ant"/>
    <depend project="bsf"/>
    <depend project="xml-xerces"/>

    <home nested="build/rhino_gump"/>
    <jar name="js.jar"/>
  </project>

</module>
