<module name="rhino">

  <url href="http://www.mozilla.org/rhino/"/>
  <description>
    JavaScript for Java
  </description>

  <cvs repository="mozilla" module="mozilla/js/rhino"/>

  <project name="rhino">
    <package>org.mozilla.classfile</package>
    <package>org.mozilla.javascript</package>

    <ant target="dist">
      <property name="version" value="_@@DATE@@"/>
    </ant>

    <depend project="jakarta-ant"/>
    <depend project="bsf"/>
    <depend project="xml-xerces"/>

    <home nested="build/rhino_@@DATE@@"/>
    <jar name="js.jar"/>
    <javadoc nested="build/rhino_@@DATE@@/docs/apidocs"/>
  </project>

</module>
