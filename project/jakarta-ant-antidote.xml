<module name="jakarta-ant-antidote">

  <url href="http://ant.apache.org/index.html"/>
  <description>
    GUI frontend to Ant
  </description>

  <cvs repository="jakarta"/>

  <project name="jakarta-ant-antidote">
    <package>org.apache.tools.ant.gui</package>

    <ant target="internal_dist"/>
    <depend project="bootstrap-ant"/>
    <depend project="xml-xerces"/>
    <home nested="dist"/>
    <jar name="lib/antidote.jar"/>
    <javadoc nested="dist/docs/api" module="jakarta-ant"/>

    <nag from="Diane Holt &lt;holtdl@apache.org&gt;"
         to="ant-dev@jakarta.apache.org"/>
  </project>

</module>
