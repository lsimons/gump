<module name="importscrubber">

  <url href="http://importscrubber.sourceforge.net/"/>
  <description>
    A utility to neaten up the import statements in a Java source code file.
  </description>

  <cvs repository="sourceforge" host-prefix="cvs.importscrubber" 
       dir="importscrubber"/>
  
  <project name="importscrubber">
    <ant basedir="etc"/>
    <depend project="jakarta-ant"/>
    <depend project="xml-xerces"/>
    <depend project="cfparse"/>
    <depend project="junit"/>
    <jar name="build/importscrubber.jar"/>
  </project>

  <project name="cfparse">
    <url href="http://www.alphaworks.ibm.com/tech/cfparse"/>
    <description>
      Class file parser
    </description>
    <jar name="lib/cfparse.jar"/>
  </project>

</module>
