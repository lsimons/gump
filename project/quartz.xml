<module name="quartz">

  <url href="http://quartz.sourceforge.net/"/>
  <description>
    Job Scheduler
  </description>

  <cvs repository="sourceforge" host-prefix="cvs.quartz" dir="quartz"
	  module="Quartz"/>

  <project name="quartz">
    <package>org.quartz</package>

    <ant>
      <property name="version" value="@@DATE@@"/>
    </ant>

    <depend project="ant" inherit="runtime"/>
    <depend project="xml-xerces"/>
    <depend project="jdbc"/>
    <depend project="jta"/>
    <depend project="commons-collections" />
    <depend project="commons-dbcp" />
    <depend project="commons-logging" />
    <depend project="commons-pool" />
    <depend project="jakarta-log4j"/>
    <depend project="jakarta-servletapi-5-servlet"/>
    <depend project="jakarta-struts"/>
    <depend project="junit"/>
    <jar name="lib/quartz.jar"/>

    <license name="license.txt"/>

    <nag to="jhouserizer@part.net"
	    from="Sam Ruby &lt;rubys@apache.org&gt;"/>
  </project>

</module>
