<module name="jetty">

  <url  href="http://jetty.mortbay.org/jetty/index.html"/>
  <description>
    Java HTTP Servlet Server
  </description>
  <redistributable/>

  <cvs repository="sourceforge" host-prefix="cvs.jetty" dir="jetty"
    module="Jetty"/>
  
  <project name="jetty">
    <package>org.mortbay</package>

    <ant target="jetty.jar"/>

    <depend project="ant"/>
    <depend project="commons-logging" runtime="true"/>
    <depend project="xml-xerces"/>
    <depend project="mx4j"/>
    <depend project="mx4j-tools"/>
    <work nested="classes"/>

    <jar name="lib/org.mortbay.jetty.jar"/>
    <license name="LICENSE.html"/>
  </project>

</module>
