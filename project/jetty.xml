<module name="jetty">

  <url href="http://jetty.mortbay.com/jetty/index.html"/>
  <description>
     HTTP Server and Servlet container
  </description>

  <cvs repository="sourceforge" 
    host-prefix="cvs.jetty" dir="jetty" module="Jetty"/>
  
  <project name="jetty">
    <ant/>
    <depend project="jakarta-ant"/>
    <depend project="xml-xerces"/>
    <depend project="jsse"/>
    <work nested="classes"/>
    <jar name="lib/org.mortbay.jetty.jar"/>
  </project>

</module>
