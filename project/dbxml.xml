<module name="dbxml">

  <url href="http://www.dbxml.org/"/>
  <description>
    The dbXML Core is a native XML database
  </description>

  <cvs repository="dbxml" module="dbXML-Core"/>
  
  <project name="dbxml">
    <ant basedir="java"/>
    <depend project="jakarta-ant"/>
    <depend project="xml-xerces"/>
    <depend project="jacorb"/>
    <depend project="xmldb"/>
    <depend project="juggernaut"/>
    <depend project="infozone"/>
    <depend project="xml-xalan2"/>
    <jar name="java/lib/dbXML.jar"/>
  </project>

  <project name="jacorb">
    <url href="http://jacorb.inf.fu-berlin.de/"/>
    <home nested="java/lib"/>
    <jar name="jacorb-1.1.jar"/>
    <jar name="idl-1.1.jar"/>
  </project>

  <project name="xmldb">
    <url href="http://www.xmldb.org/xapi/index.html"/>
    <home nested="java/lib"/>
    <jar name="xmldb.jar"/>
    <jar name="xmldb-sdk.jar"/>
  </project>

  <project name="juggernaut">
    <url href="http://www.dbxmlgroup.com/juggernaut.xml"/>
    <home nested="java/lib"/>
    <jar name="juggernaut-1.0.jar"/>
  </project>

  <project name="infozone">
    <url href="http://infozone-group.org/"/>
    <home nested="java/lib"/>
    <jar name="infozone-lexus.jar"/>
    <jar name="infozone-tools.jar"/>
  </project>

</module>
