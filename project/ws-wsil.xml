<module name="ws-wsil">

  <url href="http://cvs.apache.org/viewcvs.cgi/*checkout*/ws-wsil/java/README.htm"/>
  <description>
    Web Services Inspection Language 
  </description>

  <cvs repository="ws"/>

  <!-- alias -->

  <project name="xml-axis-wsil">
    <depend project="ws-wsil" inherit="jars"/>
  </project>

  <!-- build -->

  <project name="ws-wsil">
    <package>org.apache.wsil</package>

    <ant basedir="java" target="all"/>

    <depend project="ant" inherit="runtime"/>
    <depend project="xml-xerces"/>
    <depend project="uddi4j"/>
    <depend project="wsdl4j"/>

    <work nested="java/build/classes"/>

    <nag from="Sam Ruby &lt;rubys@us.ibm.com&gt;"
         to="axis-dev@ws.apache.org">
      <regexp  subject="Build Failure - WSIL"/>
    </nag>
  </project>

</module>
