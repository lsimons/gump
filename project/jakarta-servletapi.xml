<module name="jakarta-servletapi">

  <description>
    Servlet 2.2 and JSP 1.1 API
  </description>

  <cvs repository="jakarta"/>
  
  <project name="jakarta-servletapi">
    <package>javax.servlet</package>

    <ant target="dist"/>
    <depend project="xml-xerces"/>
    <depend project="jakarta-ant" inherit="runtime"/>
    <home nested="dist"/>
    <jar  name="lib/servlet.jar"/>
    <javadoc nested="dist/docs/api" module="jakarta-tomcat"/>
  </project>

</module>
