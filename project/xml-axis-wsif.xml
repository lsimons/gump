<module name="xml-axis-wsif">

  <url href="http://xml.apache.org/axis/"/>
  <description>
    Web Services Invocation Framework
  </description>

  <cvs repository="xml"/>

  <!-- build -->

  <project name="xml-axis-wsif">
    <package>org.apache.wsif</package>

    <ant basedir="java" target="bindist">
      <depend property="wsdl4j_jar" project="wsdl4j" id="wsdl4j"/>
      <depend property="qname_jar"  project="wsdl4j" id="qname"/>
    </ant>

    <depend project="jakarta-ant" inherit="runtime"/>
    <depend project="xml-xerces"/>
    <depend project="bsf"/>
    <depend project="ejb"/>
    <depend project="javamail"/>
    <depend project="jaf"/>
    <depend project="commons-logging"/>
    <depend project="jms"/>
    <depend project="wsdl4j"/>
    <depend project="xml-axis"/>
    <depend project="xml-soap"/>
    <depend project="j2ee-connector"/>

    <work nested="java/build/classes"/>

    <javadoc nested="java/build/javadocs"/>

    <nag from="Sam Ruby &lt;rubys@us.ibm.com&gt;"
         to="axis-dev@xml.apache.org"/>
  </project>

  <project name="soaprmi">
    <url href="http://www.extreme.indiana.edu/xgws/xsoap/"/>
    <jar name="java/lib/soaprmi11/soaprmi11.jar"/>
  </project>

</module>
