<!-- similar to gump2.xml, but no "tag=" attribute in the module (using HEAD) -->

<module name="jakarta-tomcat-jasper_tc5">

  <description>
    JavaServer Pages JSP 2.0 implementation (for Tomcat 5.x)
  </description>
  <url href="http://jakarta.apache.org/tomcat/index.html"/>

  <cvs repository="jakarta" module="jakarta-tomcat-jasper"/>
  <project name="jakarta-tomcat-jasper_tc5">
    <ant basedir="jasper2" target="dist">
      <depend property="ant.jar" project="ant" id="ant"/>
      <depend property="commons-el.jar" project="commons-el"/>
      <depend property="servlet-api.jar"
              project="jakarta-servletapi-5-servlet"/>
      <depend property="jsp-api.jar" project="jakarta-servletapi-5-jsp"/>
    </ant>
    <home nested="jasper2" />
    <jar name="dist/shared/lib/jasper-compiler.jar"/>
    <jar name="dist/shared/lib/jasper-runtime.jar"/>

    <depend project="xml-xerces"/>
    <depend project="commons-logging"/>
  </project>
</module>
