<module name="openjms">

  <url href="http://openjms.exolab.org/"/>
  <description>
    Java Message Service
  </description>

  <cvs repository="exolab" host-prefix="openjms" dir="openjms"/>
  
  <project name="openjms">
    <ant buildfile="src/build.xml" target="jar"/>
    <depend project="jakarta-ant"/>
    <depend project="xml-xerces"/>
    <depend project="openjms.ant.extensions"/>
    <depend project="antlr"/>
    <depend project="castor"/>
    <depend project="openjms.exolab.core"/>
    <depend project="jms"/>
    <depend project="jtf"/> <!-- from castor -->
    <depend project="jta"/>
    <depend project="jdbc"/>
    <depend project="tyrex"/>
    <depend project="jakarta-regexp"/>
  </project>

  <project name="antlr">
    <jar name="lib/antlrall_2.7.1.jar"/>
    <jar name="lib/antlr_2.7.1.jar"/>
  </project>

  <project name="openjms.ant.extensions">
    <jar name="lib/ant-extension.jar"/>
  </project>

  <project name="openjms.exolab.core">
    <jar name="lib/exolabcore-0.3.1.jar"/>
  </project>

</module>
