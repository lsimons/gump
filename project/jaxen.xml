<?xml version="1.0" encoding="UTF-8"?>

<module name="jaxen">
  <description>Universal Java XPath Engine</description>
  <url href="http://jaxen.org/">
  </url>
  <cvs module="jaxen" repository="werken" dir="jaxen">
  </cvs>
  <project name="jaxen">
    <ant buildfile="build.xml" target="dist">
      <property name="final.name" value="jaxen-@@DATE@@">
      </property>
    </ant>
    <package>org.jaxen</package>
    <!-- 
        DOM4J includes jaxen source in its build, so that piece is gump'ified there
    <depend project="dom4j">
    </depend>
    -->
    <depend project="jakarta-ant">
    </depend>
    <depend project="jdom">
    </depend>
    <depend project="junit">
    </depend>
    <depend project="saxpath">
    </depend>
    <depend project="xml-apis">
    </depend>
    <depend project="xml-xerces">
    </depend>
    <work nested="target/classes">
    </work>
    <home nested="target">
    </home>
    <jar name="jaxen-@@DATE@@.jar">
    </jar>
    <javadoc module="jaxen" nested="target/docs/apidocs">
    </javadoc>
    <nag to="jaxen-interest@lists.codehaus.org" from="jaxen development &lt;jaxen-interest@lists.codehaus.org&gt;">
    </nag>
  </project>
</module>