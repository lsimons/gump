<module name="jakarta-avalon-testlet">

  <url href="http://jakarta.apache.org/avalon/"/>
  <description>
    A Unit testing framework
  </description>

  <cvs repository="jakarta"/>
  
  <project name="jakarta-avalon-testlet">
    <ant target="dist"/>
    <depend project="jakarta-ant"/>
    <depend project="xml-xerces"/>
    <depend project="jakarta-avalon-clutil"/>
    <home nested="build/lib"/>
    <jar name="testlet.jar"/>
    <javadoc nested="build/javadocs" module="jakarta-avalon"/>
  </project>

</module>
