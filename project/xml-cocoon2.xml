<module name="xml-cocoon2">

  <url href="http://xml.apache.org/cocoon/cocoon2.html"/>
  <description>
    Java publishing framework - version 2
  </description>

  <cvs repository="xml"/>
  
  <project name="xml-cocoon2">
    <ant target="dist">
      <depend property="logkit.jar"    project="dist-avalon-logkit"/>
      <depend property="avalonapi.jar" project="jakarta-avalon"/>
    </ant>
    <depend project="jakarta-ant"/>
    <depend project="xml-xerces"/>
    <depend project="xml-xalan2"/>
    <depend project="xml-batik"/>
    <depend project="jakarta-avalon-excalibur"/>
    <depend project="jakarta-regexp"/>
    <depend project="jdom"/>
    <depend project="maybeupload"/>
    <depend project="bsf"/>
    <depend project="jakarta-velocity"/>
    <depend project="resolver"/>
    <option project="xml-fop"/>
    <option project="jtidy"/>
    <option project="phpservlt"/>
    <option project="jakarta-servletapi-4"/>
    <option project="xt"/>
    <option project="xml-batik"/>
    <option project="rhino"/>
    <option project="jstyle"/>
    <work nested="bin"/>
  </project>

  <project name="jstyle">
    <url href="http://astyle.sourceforge.net/"/>
    <description>
      Predecessor to Artistic Style?
    </description>
    <jar name="lib/jstyle.jar"/>
  </project>

  <project name="xt">
    <url href="http://www.jclark.com/xml/xt.html"/>
    <description>
      XSL Transformation
    </description>
    <jar name="lib/xt.jar"/>
  </project>

  <project name="maybeupload">
    <url href="http://www.weft.co.uk/library/maybeupload/"/>
    <description>
      Maybe Upload
    </description>
    <jar name="lib/maybeupload.jar"/>
  </project>

  <project name="resolver">
    <url href="http://www.sun.com/xml/developers/resolver/"/>
    <description>
      XML Entity and URI resolvers
    </description>
    <jar name="lib/resolver.jar"/>
  </project>

</module>
