<module name="jgen">

  <url href="http://sourceforge.net/projects/jgen/"/>
  <description>
    Flash 5 Content Generation
  </description>

  <cvs repository="sourceforge" host-prefix="cvs.jgen" dir="jgen"/>
  
  <project name="jgen">
    <package>com.iv.flash</package>

    <ant target="jar"/>

    <depend project="ant" inherit="runtime"/>
    <depend project="xml-xerces"/>
    
    <depend project="commons-jxpath"/>
    <depend project="jakarta-servletapi-4"/>
    <depend project="jakarta-log4j"/>
    <depend project="jaxen"/>
    <depend project="junit"/>
    <depend project="commons-jexl"/>
    <depend project="xml-batik"/>
    <depend project="xml-fop-maintenance" inherit="runtime"/>
    <depend project="xml-xalan2"/>
    <depend project="xml-apis"/>
    <depend project="rhino"/>
    <depend project="resin"/>
    <depend project="enterprisedt.ftp"/>

    <jar name="lib/jgen.jar"/>
    <license name="LICENSE"/>
  </project>

  <project name="resin">
    <url href="http://www.caucho.com/"/>
    <package>com.caucho</package>
    <jar name="lib/resin.jar"/>
  </project>

  <project name="enterprisedt.ftp">
    <url href="http://www.enterprisedt.com/downloads/ftp.html"/>
    <package>com.enterprisedt.net.ftp</package>
    <jar name="lib/ftp.jar"/>
  </project>
</module>
