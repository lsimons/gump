<module name="xmlunit">

  <url href="http://xmlunit.sourceforge.net/"/>
  <description>
    JUnit extension for XML unit tests.
  </description>

  <cvs repository="sourceforge" host-prefix="cvs.xmlunit" dir="xmlunit"/>
  
  <project name="xmlunit">
    <package>org.custommonkey.xmlunit</package>
    <ant target="dist">
      <property name="xmlunit.version" value="@@DATE@@" />
      <property name="xmlxsl.lib" value="."/>
      <depend project="junit" property="junit.lib"/>
      <property name="test.report.dir" value="testreports"/>
    </ant>
    <depend project="ant" inherit="runtime"/>
    <depend project="xml-xerces"/>
    <depend project="xml-xalan2"/>
    <work nested="classes"/>
    <jar name="lib/xmlunit-@@DATE@@.jar"/>
    <license name="LICENSE.txt"/>
    <nag from="Sam Ruby &lt;rubys@us.ibm.com&gt;"
         to="timbacon@users.sourceforge.net"/>
  </project>

</module>
