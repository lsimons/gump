<module name="ws-xmlrpc">

  <url href="http://ws.apache.org/xmlrpc/"/>
  <description>
    A Java implementation of XML-RPC
  </description>

  <cvs repository="ws"/>

  <project name="ws-xmlrpc">
    <package>org.apache.xmlrpc</package>

    <ant>
      <property name="final.name" value="xmlrpc-@@DATE@@"/>
    </ant>

    <depend project="xml-xerces"/>
    <depend project="commons-codec"/>
    <depend project="commons-httpclient-2.0-branch"/>
    <depend project="ant" inherit="runtime"/>
    <option project="jakarta-servletapi"/>
    <option project="jsse"/>
    <!-- depend project="fesi"/ -->

    <home nested="target"/>
    <jar name="xmlrpc-@@DATE@@.jar"/>

    <license name="LICENSE.txt"/>

    <nag from="Sam Ruby &lt;rubys@us.ibm.com&gt;"
         to="xmlrpc-dev@ws.apache.org"/>
  </project>

</module>
