<module name="jdom">

  <url href="http://www.jdom.org"/>
  <description>
    Java XML API
  </description>

  <cvs repository="jdom"/>

  <project name="jdom">
    <package>org.jdom</package>
    <redistributable/>

    <ant/>
    <depend project="jakarta-ant" inherit="runtime"/>
    <depend project="xml-xerces"/>
    <depend project="xml-xalan2"/>
    <home nested="build"/>
    <jar name="jdom.jar" id="jdom"/>
    <license name="LICENSE.txt"/>
  </project>

  <project name="saxpath">
    <jar name="lib/saxpath.jar"/>
  </project>

</module>
