<module name="jdom">

  <url href="http://www.jdom.org"/>
  <description>
    Java XML API
  </description>

  <package>org.jdom</package>

  <cvs repository="jdom"/>

  <project name="jdom">
    <ant/>
    <depend project="jakarta-ant"/>
    <depend project="xml-xerces"/>
    <depend project="xml-xalan2"/>
    <home nested="build"/>
    <jar name="jdom.jar" id="jdom"/>
  </project>

</module>
