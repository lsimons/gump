<module name="webwork">

  <url href="http://opensymphony.com/webwork/"/>
  <description>
    WebWork web application framework.
  </description>
  <redistributable/>

  <cvs repository="sourceforge" host-prefix="cvs.opensymphony" 
       dir="opensymphony" module="webwork"/>
  
  <project name="webwork">
    <package>webwork</package>

    <ant/>
    <depend project="ant" inherit="runtime"/>
    <depend project="xml-xerces"/>
    <depend project="javacc"/>
    <depend project="jakarta-log4j"/>
    <depend project="jakarta-servletapi-4"/>
    <depend project="jakarta-velocity"/>
    <depend project="bsf"/>
    <depend project="jdom"/>
    <depend project="jasperreports"/>
    <depend project="db40"/>
    <depend project="ws-axis"/>
    <depend project="cos"/>
    <depend project="commons-collections"/>
    <depend project="commons-logging"/>
    <depend project="mockobjects"/>
    <depend project="mockobjects-j2ee-13"/>
    <depend project="multipartrequest"/>
    <depend project="junit"/>
    <depend project="httpunit"/>
    <work nested="lib/build/JavaCC.zip"/>
    <jar id="webwork" name="build/result/web/WEB-INF/lib/webwork.jar"/>
    <license name="src/docs/manual/license.txt"/>
  </project>

  <project name="db40">
    <package>com.db4o</package>
    <url href="http://www.db4o.com/"/>
    <description>
      Object Database Engine 
    </description>
    <home nested="lib/examples"/>
    <jar name="db4o.jar"/>
  </project>

  <project name="jasperreports">
    <package>dori.jasper.engine</package>
    <url href="http://jasperreports.sourceforge.net"/>
    <description>
      
    </description>
    <home nested="lib/view"/>
    <jar name="jasperreports.jar"/>
  </project>
</module>
