<module name="hsqldb">

  <url href="http://hsqldb.sourceforge.net/"/>
  <description>
    hsql Database Engine
  </description>

  <cvs repository="sourceforge" host-prefix="cvs.hsqldb" dir="hsqldb" 
       module="hsqldb-dev"/>
  
  <project name="hsqldb">
    <package>org.hsqldb</package>

    <mkdir dir="classes"/>

    <ant basedir="build" target="jar"/>

    <depend project="ant" inherit="runtime"/>
    <depend project="xml-xerces"/>
    <depend project="jakarta-servletapi-4"/>

    <work nested="classes"/>

    <jar name="lib/hsqldb.jar" id="hsqldb"/>
  </project>

</module>
