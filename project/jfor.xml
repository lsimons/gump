<?xml version="1.0" encoding="UTF-8"?>
<module name="jfor">

  <url href="http://www.jfor.org"/>
  <description>
    Java xsl-FO to Rtf converter
  </description>

  <cvs dir="jfor" host-prefix="cvs.jfor" repository="sourceforge"/>
  
  <project name="jfor">
    <ant>
      <property name="dist.jar" value="dist/lib/jfor-@@DATE@@.jar"/>
    </ant>
    <depend project="jakarta-ant" inherit="runtime"/>
    <depend project="xml-xerces"/>
    <depend project="jakarta-avalon-logkit"/>
    <home nested="dist"/>
    <jar name="lib/jfor-@@DATE@@.jar"/>
  </project>

</module>