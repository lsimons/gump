<module name="cruisecontrol">

  <url  href="http://cruisecontrol.sourceforge.net"/>
  <description>
     tool for setting up a continuous build process
  </description>

  <cvs repository="sourceforge" host-prefix="cvs.cruisecontrol" 
                                dir="cruisecontrol"/>
  
  <project name="cruisecontrol">
    <ant/>
    <depend project="dist-ant"/>
    <depend project="xml-xerces"/>
    <depend project="xml-xalan2"/>
    <depend project="jakarta-servletapi-4"/>
    <depend project="javamail"/>
    <depend project="jakarta-oro"/>
    <depend project="starteam"/>
    <depend project="jmx"/>
    <depend project="junit"/>

    <work nested="classes"/>
  </project>

  <project name="starteam">
    <home nested="lib"/>
    <jar name="starteam-sdk-interfaceonly.jar"/>
  </project>

</module>
