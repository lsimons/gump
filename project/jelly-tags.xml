    <description>This is a Jelly interface for unit testing XML applications.</description>

    <ant basedir="jelly/jelly-tags/xmlunit" target="jar">
      <property name="final.name"  value="commons-jelly-tags-xmlunit-@@DATE@@"/>
    </ant>

    <depend project="commons-jelly" inherit="all"/>
    <depend project="commons-jelly-tags-xml"/>
    <depend project="commons-jelly-tags-junit"/>
    
    <depend project="xmlunit"/>
    <depend project="xml-xalan2"/>

    <work nested="jelly/jelly-tags/xmlunit/target/classes"/>
    <work nested="jelly/jelly-tags/xmlunit/target/test-classes"/>

    <jar name="jelly/jelly-tags/xmlunit/target/commons-jelly-tags-xmlunit-@@DATE@@.jar"/>
 
    <nag from="Morgan Delagrange &lt;mdelagra@yahoo.com&gt;"
         to="commons-dev@jakarta.apache.org"/>
  </project>  
  
</module>
