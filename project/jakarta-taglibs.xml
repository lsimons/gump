<project name="jakarta-taglibs">

  <url href="http://jakarta.apache.org/taglibs/index.html"/>
  <description>
    JSP Taglibs
  </description>

  <cvs repository="jakarta"/>

  <ant target="dist">
    <property name="oro.jar"
       reference="jarpath" id="oro" project="jakarta-oro"/>
    <property name="xalan.jar"
       reference="jarpath" id="xalan" project="xml-xalan2"/>
    <property name="env.SERVLET_JAR"
       reference="jarpath" id="servlet" project="jakarta-servletapi-4"/>
  </ant>

  <depend project="xml-xerces"/>
  <depend project="xml-xalan2"/>
  <depend project="jakarta-oro"/>
  <depend project="jakarta-ant"/>
  <depend project="jakarta-servletapi-4"/>
  <depend project="jdbc"/>
  <option project="bsf"/>
  <option project="xml-xalan2"/>
  <option project="javamail"/>
  <depend project="dom4j"/>
  <depend project="jakarta-log4j"/>
  <depend project="jaf"/>

</project>
