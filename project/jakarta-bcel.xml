<module name="jakarta-bcel">

  <url href="http://jakarta.apache.org/bcel"/>
  <description>
     Byte Code Engineering Library
  </description>

  <cvs repository="jakarta"/>
  
  <project name="jakarta-bcel">
    <package>org.apache.bcel</package>

    <ant/>

    <depend project="bootstrap-ant" inherit="runtime"/>
    <depend project="xml-xerces"/>
    <depend project="jakarta-regexp" runtime="true"/>

    <jar name="bin/bcel.jar"/>

    <license name="LICENSE.txt"/>

    <nag from="Jason van Zyl &lt;jvanzyl@apache.org&gt;"
         to="bcel-dev@jakarta.apache.org"/>
  </project>

</module>
