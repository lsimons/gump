<!-- $Id: javassist.xml,v 1.3 2004/02/27 09:22:56 bodewig Exp $ -->

<module name="javassist">

  <url href="http://www.jboss.org/index.html?module=html&amp;op=userdisplay&amp;id=developers/projects/javassist"/>
  <description>
    Bytecode enhancement and AOP framework
  </description>

  <cvs repository="sourceforge" host-prefix="cvs.jboss" dir="jboss"
	  module="javassist"/>

  <project name="javassist">
    <package>javassist</package>

    <ant target="jar">
      <property name="version" value="@@DATE@@"/>
    </ant>

    <depend project="ant" inherit="runtime"/>
    
    <jar name="javassist.jar"/>

    <license name="License.html"/>

    <nag to="hlship@apache.org" from="Sam Ruby &lt;rubys@apache.org&gt;"/>
  </project>

</module>
