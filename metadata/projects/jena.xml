<module name="jena">

  <url  href="http://jena.sourceforge.net/index.html"/>
  <description>
    Jena is a Java framework for building Semantic Web applications. It provides a programmatic environment for RDF, RDFS and OWL, including a rule-based inference engine.
  </description>
  <redistributable/>

  <cvs repository="sourceforge"  dir="jena"/>
  
  <project name="jena">
    <package>com.hp.hpl.jena</package>

    <ant>
      <property name="dist" reference="home" project="jena"/>
    </ant>

    <depend project="ant"/>
    <depend project="antlr"/>
    <depend project="xml-xerces"/>
    <depend project="jakarta-oro"/>
    <depend project="junit"/>
    <depend project="concurrent"/>
    <depend project="icu4j"/>
	
    <jar name="jena.jar" id="jena"/>
  </project>

</module>
